<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="mapping.RuoloMapping">

	<insert id="insert" parameterType="models.Ruolo" useGeneratedKeys="true" keyProperty="idRuolo" keyColumn="IDRUOLO">
		INSERT INTO ruolo (RUOLO,IDRUOLO)
		VALUES(
			#{ruolo},
			#{idRuolo}
		)
	</insert>
</mapper>